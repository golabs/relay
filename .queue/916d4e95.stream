w style={styles.microsoftLogo}>\n                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.red }]} />\n                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.green }]} />\n                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.blue }]} />\n                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.yellow }]} />\n              </View>\n              <Text style={styles.ssoButtonText}>Sign in with Microsoft</Text>\n            </>\n          )}\n          {buttonState === 'loading' && (\n            <Text style={styles.ssoButtonText}>Authenticating...</Text>\n          )}\n          {buttonState === 'success' && (\n            <Text style={styles.ssoButtonText}>Welcome, Brett!</Text>\n          )}\n\n          {/* Progress bar */}\n          {buttonState === 'loading' && (\n            <Animated.View\n              style={[\n                styles.progressBar,\n                {\n                  width: progressAnim.interpolate({\n                    inputRange: [0, 1],\n                    outputRange: ['0%', '100%'],\n                  }),\n                },\n              ]}\n            />\n          )}\n        </TouchableOpacity>\n\n        <Text style={styles.securedText}>Secured by Microsoft Entra ID</Text>\n\n        <View style={styles.footer}>\n          <Text style={styles.footerText}>\n            <Text style={styles.footerHighlight}>Walter AI</Text> — Powered by Azure OpenAI{'\\n'}\n            © 2026 Walter Insurance Technologies\n          </Text>\n        </View>\n      </Animated.View>\n    </View>\n  );\n}\n\nconst styles = StyleSheet.create({\n  container: {\n    flex: 1,\n    backgroundColor: Colors.background,\n    justifyContent: 'center',\n    alignItems: 'center',\n  },\n  content: {\n    flex: 1,\n    justifyContent: 'center',\n    alignItems: 'center',\n    paddingHorizontal: Spacing.lg,\n  },\n  logoContainer: {\n    justifyContent: 'center',\n    alignItems: 'center',\n    marginBottom: 16,\n  },\n  logoImage: {\n    width: 100,\n    height: 100,\n  },\n  brandName: {\n    fontSize: 32,\n    fontWeight: Fonts.extrabold,\n    color: Colors.text,\n    letterSpacing: -0.5,\n    marginBottom: 4,\n  },\n  brandTagline: {\n    fontSize: 14,\n    fontWeight: Fonts.medium,\n    color: Colors.primaryLight,\n    letterSpacing: 3,\n    textTransform: 'uppercase',\n    marginTop: 4,\n  },\n  brandSubtitle: {\n    fontSize: 15,\n    fontWeight: Fonts.regular,\n    color: Colors.text50,\n    textAlign: 'center',\n    marginTop: 8,\n    lineHeight: 22,\n  },\n  divider: {\n    width: 60,\n    height: 2,\n    marginVertical: 40,\n  },\n  buttonContainer: {\n    width: '100%',\n    paddingHorizontal: Spacing.xl,\n    paddingBottom: Spacing.xxl,\n    alignItems: 'center',\n  },\n  ssoButton: {\n    flexDirection: 'row',\n    alignItems: 'center',\n    justifyContent: 'center',\n    backgroundColor: 'rgba(124, 58, 237, 0.1)',\n    borderWidth: 1,\n    borderColor: Colors.primaryBorder,\n    borderRadius: 16,\n    paddingVertical: 18,\n    paddingHorizontal: Spacing.lg,\n    width: '100%',\n    overflow: 'hidden',\n  },\n  ssoButtonSuccess: {\n    borderColor: Colors.green,\n    backgroundColor: 'rgba(34, 197, 94, 0.1)',\n  },\n  microsoftLogo: {\n    width: 20,\n    height: 20,\n    flexDirection: 'row',\n    flexWrap: 'wrap',\n    gap: 2,\n    marginRight: 12,\n  },\n  msSquare: {\n    width: 9,\n    height: 9,\n  },\n  ssoButtonText: {\n    fontSize: 16,\n    fontWeight: Fonts.semibold,\n    color: Colors.text,\n  },\n  progressBar: {\n    position: 'absolute',\n    bottom: 0,\n    left: 0,\n    height: 3,\n    backgroundColor: Colors.primary,\n  },\n  securedText: {\n    fontSize: 12,\n    color: Colors.text30,\n    marginTop: 24,\n  },\n  footer: {\n    marginTop: 40,\n    alignItems: 'center',\n  },\n  footerText: {\n    fontSize: 11,\n    color: Colors.text20,\n    textAlign: 'center',\n    lineHeight: 18,\n