Animated.View>\n   110→\n   111→      {/* Login button */}\n   112→      <Animated.View style={[styles.buttonContainer, { opacity: fadeAnim }]}>\n   113→        <TouchableOpacity\n   114→          style={[\n   115→            styles.ssoButton,\n   116→            buttonState === 'success' && styles.ssoButtonSuccess,\n   117→          ]}\n   118→          onPress={handleLogin}\n   119→          disabled={buttonState !== 'idle'}\n   120→          activeOpacity={0.8}\n   121→        >\n   122→          {buttonState === 'idle' && (\n   123→            <>\n   124→              <View style={styles.microsoftLogo}>\n   125→                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.red }]} />\n   126→                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.green }]} />\n   127→                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.blue }]} />\n   128→                <View style={[styles.msSquare, { backgroundColor: Colors.microsoft.yellow }]} />\n   129→              </View>\n   130→              <Text style={styles.ssoButtonText}>Sign in with Microsoft</Text>\n   131→            </>\n   132→          )}\n   133→          {buttonState === 'loading' && (\n   134→            <Text style={styles.ssoButtonText}>Authenticating...</Text>\n   135→          )}\n   136→          {buttonState === 'success' && (\n   137→            <Text style={styles.ssoButtonText}>Welcome, Brett!</Text>\n   138→          )}\n   139→\n   140→          {/* Progress bar */}\n   141→          {buttonState === 'loading' && (\n   142→            <Animated.View\n   143→              style={[\n   144→                styles.progressBar,\n   145→                {\n   146→                  width: progressAnim.interpolate({\n   147→                    inputRange: [0, 1],\n   148→                    outputRange: ['0%', '100%'],\n   149→                  }),\n   150→                },\n   151→              ]}\n   152→            />\n   1