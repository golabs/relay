{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_014jKaHdGmvKTvodGYLQPwGJ","type":"tool_result","content":"     1→/**\n     2→ * Tasks.tsx - Task Management Page\n     3→ *\n     4→ * WALTER Task Management with:\n     5→ * - Real API integration\n     6→ * - Create/Edit/Delete tasks with modals\n     7→ * - Mark complete functionality\n     8→ * - Due date support\n     9→ * - Filter by status and priority\n    10→ */\n    11→\n    12→import React, { useState, useEffect, useCallback } from 'react';\n    13→import {\n    14→  CheckCircle2,\n    15→  Clock,\n    16→  AlertCircle,\n    17→  Plus,\n    18→  Calendar,\n    19→  Trash2,\n    20→  Edit2,\n    21→  Loader2,\n    22→  X\n    23→} from 'lucide-react';\n    24→import { Button } from '@/components/ui/button';\n    25→import { Card, CardContent, CardHeader, CardTitle } from '@/components/ui/card';\n    26→import { Badge } from '@/components/ui/badge';\n    27→import { Input } from '@/components/ui/input';\n    28→import { cn } from '@/lib/utils';\n    29→import { tasksService, Task, TaskStats } from '@/services/api/tasks.service';\n    30→import { useToast } from '@/hooks/use-toast';\n    31→\n    32→// Task form for create/edit\n    33→interface TaskFormData {\n    34→  title: string;\n    35→  description: string;\n    36→  priority: 'low' | 'medium' | 'high' | 'urgent';\n    37→  status: 'pending' | 'in_progress' | 'completed' | 'cancelled';\n    38→  due_date: string;\n    39→}\n    40→\n    41→const initialFormData: TaskFormData = {\n    42→  title: '',\n    43→  description: '',\n    44→  priority: 'medium',\n    45→  status: 'pending',\n    46→  due_date: ''\n    47→};\n    48→\n    49→const Tasks: React.FC = () => {\n    50→  const { toast } = useToast();\n    51→  const [tasks, setTasks] = useState<Task[]>([]);\n    52→  const [stats, setStats] = useState<TaskStats | null>(null);\n    53→  const [loading, setLoading] = useState(true);\n    54→  const [statusFilter, setStatusFilter] = useState<string>('all');\n    55→  const [priorityFilter, setPriorityFilter] = useState<string>('all');\n    56→  const [searchTerm, setSearchTerm] = useState('');\n    57→\n    58→  // Modal state\n    59→  const [showModal, setShowModal] = useState(false);\n    60→  const [editingTask, setEditingTask] = useState<Task | null>(null);\n    61→  const [formData, setFormData] = useState<TaskFormData>(initialFormData);\n    62→  const [saving, setSaving] = useState(false);\n    63→  const [deleteConfirm, setDeleteConfirm] = useState<string | null>(null);\n    64→\n    65→  // Fetch tasks\n    66→  const fetchTasks = useCallback(async () => {\n    67→    try {\n    68→      setLoading(true);\n    69→      const params: Record<string, string> = {};\n    70→      if (statusFilter !== 'all') params.status = statusFilter;\n    71→      if (priorityFilter !== 'all') params.priority = priorityFilter;\n    72→      if (searchTerm) params.search = searchTerm;\n    73→\n    74→      const [tasksResponse, statsResponse] = await Promise.all([\n    75→        tasksService.getTasks(params),\n    76→        tasksService.getStats()\n    77→      ]);\n    78→\n    79→      setTasks(tasksResponse.tasks || []);\n    80→      setStats(statsResponse);\n    81→    } catch (error: unknown) {\n    82→      console.error('Failed to fetch tasks:', error);\n    83→      toast({\n    84→        title: 'Error',\n    85→        description: 'Failed to load tasks. Please try again.',\n    86→        variant: 'destructive'\n    87→      });\n    88→    } finally {\n    89→      setLoading(false);\n    90→    }\n    91→  }, [statusFilter, priorityFilter, searchTerm, toast]);\n    92→\n    93→  useEffect(() => {\n    94→    fetchTasks();\n    95→  }, [fetchTasks]);\n    96→\n    97→  // Group by status\n    98→  const pendingTasks = tasks.filter(t => t.status === 'pending');\n    99→  const inProgressTasks = tasks.filter(t => t.status === 'in_progress');\n   100→  const completedTasks = tasks.filter(t => t.status === 'completed');\n   101→\n   102→  // Toggle task status\n   103→  const toggleTaskStatus = async (task: Task) => {\n   104→    const newStatus = task.status === 'completed' ? 'pending' : 'completed';\n   105→    try {\n   106→      await tasksService.updateTask(task.task_id, { status: newStatus });\n   107→      toast({\n   108→        title: newStatus === 'completed' ? 'Task completed!' : 'Task reopened',\n   109→        description: task.title\n   110→      });\n   111→      fetchTasks();\n   112→    } catch (error) {\n   113→      toast({\n   114→        title: 'Error',\n   115→        description: 'Failed to update task status.',\n   116→        variant: 'destructive'\n   117→      });\n   118→    }\n   119→  };\n   120→\n   121→  // Open create modal\n   122→  const openCreateModal = () => {\n   123→    setEditingTask(null);\n   124→    setFormData(initialFormData);\n   125→    setShowModal(true);\n   126→  };\n   127→\n   128→  // Open edit modal\n   129→  const openEditModal = (task: Task) => {\n   130→    setEditingTask(task);\n   131→    setFormData({\n   132→      title: task.title,\n   133→      description: task.description || '',\n   134→      priority: task.priority,\n   135→      status: task.status,\n   136→      due_date: task.due_date ? task.due_date.split('T')[0] : ''\n   137→    });\n   138→    setShowModal(true);\n   139→  };\n   140→\n   141→  // Handle form submit\n   142→  const handleSubmit = async (e: React.FormEvent) => {\n   143→    e.preventDefault();\n   144→    if (!formData.title.trim()) {\n   145→      toast({\n   146→        title: 'Validation Error',\n   147→        description: 'Task title is required.',\n   148→        variant: 'destructive'\n   149→      });\n   150→      return;\n   151→    }\n   152→\n   153→    setSaving(true);\n   154→    try {\n   155→      const payload = {\n   156→        title: formData.title.trim(),\n   157→        description: formData.description.trim() || undefined,\n   158→        priority: formData.priority,\n   159→        status: formData.status,\n   160→        due_date: formData.due_date || undefined\n   161→      };\n   162→\n   163→      if (editingTask) {\n   164→        await tasksService.updateTask(editingTask.task_id, payload);\n   165→        toast({\n   166→          title: 'Task updated',\n   167→          description: formData.title\n   168→        });\n   169→      } else {\n   170→        await tasksService.createTask(payload);\n   171→        toast({\n   172→          title: 'Task created',\n   173→          description: formData.title\n   174→        });\n   175→      }\n   176→\n   177→      setShowModal(false);\n   178→      setEditingTask(null);\n   179→      setFormData(initialFormData);\n   180→      fetchTasks();\n   181→    } catch (error) {\n   182→      toast({\n   183→        title: 'Error',\n   184→        description: `Failed to ${editingTask ? 'update' : 'create'} task.`,\n   185→        variant: 'destructive'\n   186→      });\n   187→    } finally {\n   188→      setSaving(false);\n   189→    }\n   190→  };\n   191→\n   192→  // Handle delete\n   193→  const handleDelete = async (taskId: string) => {\n   194→    try {\n   195→      await tasksService.deleteTask(taskId);\n   196→      toast({\n   197→        title: 'Task deleted',\n   198→        description: 'The task has been removed.'\n   199→      });\n   200→      setDeleteConfirm(null);\n   201→      fetchTasks();\n   202→    } catch (error) {\n   203→      toast({\n   204→        title: 'Error',\n   205→        description: 'Failed to delete task.',\n   206→        variant: 'destructive'\n   207→      });\n   208→    }\n   209→  };\n   210→\n   211→  const getPriorityBadge = (priority: string) => {\n   212→    const config: Record<string, { color: string }> = {\n   213→      urgent: { color: 'bg-red-600/20 text-red-600 border-red-500/30' },\n   214→      high: { color: 'bg-destructive/20 text-destructive border-destructive/30' },\n   215→      medium: