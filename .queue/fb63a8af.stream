n            0%, 100% { opacity: 1; }\n            50% { opacity: 0.5; }\n        }\n\n        /* Main layout */\n        .app-container {\n            display: flex;\n            padding-top: 60px;\n            min-height: 100vh;\n        }\n\n        /* Sidebar */\n        .sidebar {\n            width: 280px;\n            background: var(--bg-panel);\n            border-right: 1px solid var(--border);\n            padding: 1.5rem 0;\n            overflow-y: auto;\n            position: fixed;\n            top: 60px;\n            left: 0;\n            bottom: 0;\n        }\n\n        .sidebar-section {\n            margin-bottom: 1.5rem;\n        }\n\n        .sidebar-title {\n            font-family: 'Orbitron', sans-serif;\n            font-size: 0.65rem;\n            font-weight: 500;\n            letter-spacing: 0.2em;\n            text-transform: uppercase;\n            color: var(--text-dim);\n            padding: 0 1.5rem;\n            margin-bottom: 0.75rem;\n        }\n\n        .nav-item {\n            display: flex;\n            align-items: center;\n            gap: 0.75rem;\n            padding: 0.75rem 1.5rem;\n            color: var(--text-dim);\n            text-decoration: none;\n            font-size: 0.9rem;\n            font-weight: 500;\n            transition: all 0.2s ease;\n            border-left: 3px solid transparent;\n            cursor: pointer;\n        }\n\n        .nav-item:hover {\n            background: rgba(0, 212, 255, 0.05);\n            color: var(--text-primary);\n        }\n\n        .nav-item.active {\n            background: rgba(0, 212, 255, 0.08);\n            color: var(--cyan);\n            border-left-color: var(--cyan);\n        }\n\n        .nav-item svg {\n            width: 18px;\n            height: 18px;\n            opacity: 0.7;\n        }\n\n        .nav-item.active svg {\n            opacity: 1;\n        }\n\n        .nav-badge {\n            margin-left: auto;\n            padding: 0.15rem 0.5rem;\n            background: rgba(0, 212, 255, 0.15);\n            border-radius: 10px;\n            font-family: 'Share Tech Mono', monospace;\n            font-size: 0.65rem;\n            color: var(--cyan);\n        }\n\n        /* Main content */\n        .main-content {\n            flex: 1;\n            margin-left: 280px;\n            padding: 2rem;\n        }\n\n        /* Operations grid */\n        .operations-header {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            margin-bottom: 1.5rem;\n        }\n\n        .operations-title {\n            font-family: 'Orbitron', sans-serif;\n            font-weight: 500;\n            font-size: 1rem;\n            letter-spacing: 0.2em;\n            text-transform: uppercase;\n            color: var(--text-dim);\n        }\n\n        .search-box {\n            display: flex;\n            align-items: center;\n            gap: 0.5rem;\n            padding: 0.5rem 1rem;\n            background: var(--bg-card);\n            border: 1px solid var(--border);\n            border-radius: 4px;\n            width: 300px;\n        }\n\n        .search-box svg {\n            width: 16px;\n            height: 16px;\n            color: var(--text-dim);\n        }\n\n        .search-box input {\n            flex: 1;\n            background: none;\n            border: none;\n            outline: none;\n            color: var(--text-primary);\n            font-family: 'Share Tech Mono', monospace;\n            font-size: 0.8rem;\n        }\n\n        .search-box input::placeholder {\n            color: var(--text-dim);\n        }\n\n        .operations-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\n            gap: 1rem;\n        }\n\n        /* Operation card */\n        .operation-card {\n            background: var(--bg-card);\n            border: 1px solid var(--border);\n            border-radius: 8px;\n            padding: 1.25rem;\n            cursor: pointer;\n            transition: all 0.3s ease;\n            position: relative;\n            overflow: hidden;\n        }\n\n        .operation-card::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            width: 3px;\n            height: 100%;\n            background: var(--cyan);\n            transform: scaleY(0);\n            transform-origin: top;\n            transition: transform 0.3s ease;\n        }\n\n        .operation-card:hover {\n            background: var(--bg-card-hover);\n            border-color: var(--border-active);\n            transform: translateX(4px);\n        }\n\n        .operation-card:hover::before {\n            transform: scaleY(1);\n        }\n\n        .operation-card.post::before { background: var(--green); }\n        .operation-card.put::before { background: var(--yellow); }\n        .operation-card.delete::before { background: var(--red); }\n\n        .operation-header {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            margin-bottom: 0.75rem;\n        }\n\n        .method-badge {\n            font-family: 'Share Tech Mono', monospace;\n            font-size: 0.65rem;\n            font-weight: 600;\n            padding: 0.25rem 0.5rem;\n            border-radius: 3px;\n            text-transform: uppercase;\n            letter-spacing: 0.05em;\n        }\n\n        .method-badge.get {\n            background: rgba(0, 212, 255, 0.15);\n            color: var(--cyan);\n        }\n\n        .method-badge.post {\n            background: rgba(0, 255, 136, 0.15);\n            color: var(--green);\n        }\n\n        .method-badge.put {\n            background: rgba(255, 204, 0, 0.15);\n            color: var(--yellow);\n        }\n\n        .method-badge.delete {\n            background: rgba(255, 51, 51, 0.15);\n            color: var(--red);\n        }\n\n        .operation-title {\n            font-family: 'Rajdhani', sans-serif;\n            font-weight: 600;\n            font-size: 1rem;\n            color: var(--text-primary);\n            margin-bottom: 0.25rem;\n        }\n\n        .operation-endpoint {\n            font-family: 'Share Tech Mono', monospace;\n            font-size: 0.75rem;\n            color: var(--text-dim);\n            word-break: break-all;\n        }\n\n        .operation-description {\n            font-size: 0.85rem;\n            color: var(--text-dim);\n            margin-top: 0.75rem;\n            line-height: 1.4;\n        }\n\n        /* Category header */\n        .category-header {\n            display: flex;\n            align-items: center;\n            gap: 1rem;\n            margin: 2rem 0 1rem;\n            padding-bottom: 0.5rem;\n            border-bottom: 1px solid var(--border);\n        }\n\n        .category-header:first-child {\n            margin-top: 0;\n        }\n\n        .category-icon {\n            width: 28px;\n            height: 28px;\n            background: linear-gradient(135deg, rgba(0, 212, 255, 0.1) 0%, transparent 100%);\n            border: 1px solid rgba(0, 212, 255, 0.2);\n            border-radius: 6px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            color: var(--cyan);\n        }\n\n        .category-icon svg {\n            width: 16px;\n            height: 16px;\n        }\n\n        .category-title {\n            font-family: 'Orbitron', sans-serif;\n            font-size: 0.8rem;\n            font-weight: 500;\n            letter-spacing: 0.15em;\n            text-transform: uppercase;\n            color: var(--text-primary);\n        }\n\n        .category-count {\n            font-family: 'Share Tech Mono', monospace;\n            font-size: 0.7rem;\n            color: var(--text-dim);\n            margin-left: auto;\n        }\n\n        /* Mobile responsive */\n        @media (max-width: 1024px) {\n            .sidebar {\n                width: 240px;\n            }\n            .main-content {\n                margin-left: 240px;\n            }\n        }\n\n        @media (max-width: 768px) {\n            .sidebar {\n                display: none;\n            }\n            .main-content {\n                margin-left: 0;\n                padding: 1rem;\n            }\n            .operations-header {\n                flex-direction: column;\n                gap: 1rem;\n                align-items: stretch;\n            }\n            .search-box {\n                width: 100%;\n            }\n            .operations-grid {\n                grid-template-columns: 1fr;\n            }\n        }\n\n        /* Animations */\n        .fade-in {\n            opacity: 0;\n            transform: translateY(10px);\n            animation: fadeIn 0.4s ease forwards;\n        }\n\n        @keyframes fadeIn {\n            to {\n                opacity: 1;\n                transform: translateY(0);\n            }\n        }\n\n        .stagger-1 { animation-delay: 0.05s; }\n        .stagger-2 { animation-delay: 0.1s; }\n        .stagger-3 { animation-delay: 0.15s; }\n        .stagger-4 { animation-delay: 0.2s; }\n        .stagger-5 { animation-delay: 0.25s; }\n        .stagger-6 { animation-delay: 0.3s; }\n\n        /* Mobile nav toggle */\n        .mobile-nav-toggle {\n            display: none;\n            padding: 0.5rem;\n            background: var(--bg-card);\n            border: 1px solid var(--border);\n            border-radius: 4px;\n            color: var(--text-dim);\n            cursor: pointer;\n        }\n\n        @media (max-width: 768px) {\n            .mobile-nav-toggle {\n                display: flex;\n                align-items: center;\n                justify-content: center;\n            }\n        }\n\n        /* Terminal output panel */\n        .terminal-panel {\n            position: fixed;\n            bottom: 0;\n            left: 280px;\n            right: 0;\n            height: 200px;\n            background: var(--bg-panel);\n            border-top: 1px solid var(--border);\n            display: none;\n            flex-direction: column;\n            z-index: 50;\n        }\n\n        .terminal-panel.active {\n            display: flex;\n        }\n\n        .terminal-header {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            padding: 0.5rem 1rem;\n            border-bottom: 1px solid var(--border);\n        }\n\n        .terminal-title {\n            font-family: 'Share Tech Mono', monospace;\n            font-size: 0.75rem;\n            color: var(--cyan);\n            text-transform: uppercase;\n            letter-spacing: 0.1em;\n        }\n\n        .terminal-body {\n            flex: 1;\n            padding: 1rem;\n            font-family: 'Share Tech Mono', monospace;\n            font-size: 0.8rem;\n            color: var(--green);\n            overflow-y: auto;\n        }\n    </style>\n</head>\n<body>\n    <header>\n        <div class=\"header-left\">\n            <a href=\"index.html\" class=\"back-btn\">\n                <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                    <path d=\"M19 12H5M12 19l-7-7 7-7\"></path>\n                </svg>\n                Command\n            </a>\n            <div class=\"page-title\">\n                <div class=\"page-title-icon\">\n                    <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z\"></path>\n                    </svg>\n                </div>\n                <h1>INSUREDHQ</h1>\n            </div>\n        </div>\n        <div class=\"connection-status\">\n            <span class=\"connection-dot\"></span>\n            API Connected\n        </div>\n    </header>\n\n    <div class=\"app-container\">\n        <nav class=\"sidebar\">\n            <div class=\"sidebar-section\">\n                <div class=\"sidebar-title\">Operations</div>\n                <a class=\"nav-item active\" data-category=\"all\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <rect x=\"3\" y=\"3\" width=\"7\" height=\"7\"></rect>\n                        <rect x=\"14\" y=\"3\" width=\"7\" height=\"7\"></rect>\n                        <rect x=\"14\" y=\"14\" width=\"7\" height=\"7\"></rect>\n                        <rect x=\"3\" y=\"14\" width=\"7\" height=\"7\"></rect>\n                    </svg>\n                    All Operations\n                    <span class=\"nav-badge\">24</span>\n                </a>\n                <a class=\"nav-item\" data-category=\"claims\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <path d=\"M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z\"></path>\n                        <polyline points=\"14 2 14 8 20 8\"></polyline>\n                        <line x1=\"16\" y1=\"13\" x2=\"8\" y2=\"13\"></line>\n                        <line x1=\"16\" y1=\"17\" x2=\"8\" y2=\"17\"></line>\n                    </svg>\n                    Claims\n                    <span class=\"nav-badge\">6</span>\n                </a>\n                <a class=\"nav-item\" data-category=\"policies\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z\"></path>\n                    </svg>\n                    Policies\n                    <span class=\"nav-badge\">3</span>\n                </a>\n                <a class=\"nav-item\" data-category=\"clients\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <path d=\"M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2\"></path>\n                        <circle cx=\"9\" cy=\"7\" r=\"4\"></circle>\n                        <path d=\"M23 21v-2a4 4 0 0 0-3-3.87\"></path>\n                        <path d=\"M16 3.13a4 4 0 0 1 0 7.75\"></path>\n                    </svg>\n                    Clients\n                    <span class=\"nav-badge\">4</span>\n                </a>\n                <a class=\"nav-item\" data-category=\"documents\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <path d=\"M13 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V9z\"></path>\n                        <polyline points=\"13 2 13 9 20 9\"></polyline>\n                    </svg>\n                    Documents\n                    <span class=\"nav-badge\">3</span>\n                </a>\n                <a class=\"nav-item\" data-category=\"accounts\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <line x1=\"12\" y1=\"1\" x2=\"12\" y2=\"23\"></line>\n                        <path d=\"M17 5H9.5a3.5 3.5 0 0 0 0 7h5a3.5 3.5 0 0 1 0 7H6\"></path>\n                    </svg>\n                    Accounts\n                    <span class=\"nav-badge\">8</span>\n                </a>\n            </div>\n\n            <div class=\"sidebar-section\">\n                <div class=\"sidebar-title\">System</div>\n                <a class=\"nav-item\" data-category=\"sync\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <polyline points=\"23 4 23 10 17 10\"></polyline>\n                        <polyline points=\"1 20 1 14 7 14\"></polyline>\n                        <path d=\"M3.51 9a9 9 0 0 1 14.85-3.36L23 10M1 14l4.64 4.36A9 9 0 0 0 20.49 15\"></path>\n                    </svg>\n                    Sync Operations\n                </a>\n                <a class=\"nav-item\" data-category=\"health\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <path d=\"M22 12h-4l-3 9L9 3l-3 9H2\"></path>\n                    </svg>\n                    Health & Testing\n                </a>\n            </div>\n        </nav>\n\n        <main class=\"main-content\">\n            <div class=\"operations-header\">\n                <h2 class=\"operations-title\">API Operations</h2>\n                <div class=\"search-box\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <circle cx=\"11\" cy=\"11\" r=\"8\"></circle>\n                        <line x1=\"21\" y1=\"21\" x2=\"16.65\" y2=\"16.65\"></line>\n                    </svg>\n                    <input type=\"text\" placeholder=\"Search operations...\" id=\"searchInput\">\n                </div>\n            </div>\n\n            <!-- Claims Section -->\n            <div class=\"category-header fade-in\" data-category=\"claims\">\n                <div class=\"category-icon\">\n                    <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                        <path d=\"M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z\"></path>\n                        <polyline points=\"14 2 14 8 20 8\"></polyline>\n                    </svg>\n                </div>\n                <h3 class=\"category-title\">Claims Management</h3>\n                <span class=\"category-count\">6 operations</span>\n            </div>\n            <div class=\"operations-grid\" data-category=\"claims\">\n                <div class=\"operation-card fade-in stagger-1\" data-method=\"get\">\n                    <div class=\"operation-header\">\n                        <span class=\"method-badge get\">GET</span>\n                    </div>\n                    <h4 class=\"operation-title\">List Claims</h4>\n                    <code class=\"operation-endpoint\">/insuredhq/claims</code>\n                    <p class=\"operation-description\">Retrieve all claims with optional filters for status, insured name, and pagination.</p>\n                </div>\n                <div class=\"operation-card fade-in stagger-2\" data-method=\"get\">\n                    <div class=\"operation-header\">\n                        <span class=\"method-badge get\">GET</span>\n                    </div>\n                    <h4 class=\"operation-title\">Get Claim</h4>\n                    <code class=\"operation-endpoint\">/insuredhq/claims/:id</code>\n                    <p class=\"operation-description\">Retrieve details for a specific claim by ID.</p>\n                </div>\n                <div class=\"operation-card post fade-in stagger-3\" data-method=\"post\">\n                    <div class=\"operation-header\">\n                        <span class=\"method-badge post\">POST</span>\n                    </div>\n                    <h4 class=\"operation-title\">Create Claim</h4>\n                    <code class=\"operation-endpoint\">/insuredhq/claims</code>\n                    <p class=\"operation-description\">Create a new insurance claim with policy and incident details.</p>\n                </div>\n                <div class=\"operation-card put fade-in stagger-4\" data-method=\"put\">\n                    <div class=\"operation-header\">\n                        <span class=\"method-badge put\">PUT</span>\n                    </div>\n                    <h4 class=\"operation-title\">Update Claim</h4>\n                    <code class=\"operation-endpoint\">/insuredhq/claims/:id</code>\n                    <p class=\"operation-description\">Update an existing claim with new information.</p>\n                </div>\n    